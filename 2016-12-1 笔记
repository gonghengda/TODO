1.加载 404页面
import Notfound from './containers/404';
export default class Routes extends Component {
    render(){
      return(
        <Router history={browserHistory} >
          <Route path="/"               onEnter={this.verifysignin} component={Home}/>
          <Route path="/*" component={Notfound}/>
        </Router>
      )
    }
}

